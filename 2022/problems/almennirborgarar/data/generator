#!/usr/bin/env bash
PPATH=$(realpath ..)
. ../../../testdata_tools/gen.sh

use_solution bjarki_100.py

compile random_generator.py

samplegroup
sample 1
sample 2
sample 3

group gA 20 # Time simulation O(m * t_i * n)
tc gA-01 random_generator 1 100 100
tc gA-02 random_generator 2 100 100
tc gA-03 random_generator 10 100 100
tc gA-04 random_generator 100 100 100
tc gA-05 random_generator 100 0 100
tc gA-06 random_generator 100 100 1
tc gA-07 random_generator 2 100 2
tc gA-08 random_generator 100 100 10

group gB 20 # Slow simulation O(m * n)
include_group gA
tc gB-01 random_generator 1 1000 1000000000
tc gB-02 random_generator 2 1000 1000000000
tc gB-03 random_generator 10 1000 1000000000
tc gB-04 random_generator 100 1000 1000000000
tc gB-05 random_generator 1000 1000 1000000000
tc gB-06 random_generator 1000 1000 10

group gC 20 # Grouped time simulation O(m * t_i)
include_group gA
tc gC-01 random_generator 10 100000 100000
tc gC-02 random_generator 100 100000 100000
tc gC-03 random_generator 1000 100000 100000
tc gC-04 random_generator 10000 100000 100000
tc gC-05 random_generator 100000 100000 100000
tc gC-06 random_generator 200000 100000 100000
tc gC-07 random_generator 200000 100000 10

group gD 10 # Fast simulation O(m * log n)
include_group gA
include_group gB
include_group gC
tc gD-01 random_generator 10 100000 1000000000
tc gD-02 random_generator 100 100000 1000000000
tc gD-03 random_generator 1000 100000 1000000000
tc gD-04 random_generator 10000 100000 1000000000
tc gD-05 random_generator 100000 100000 1000000000
tc gD-06 random_generator 200000 100000 1000000000

group gE 30 # Binary search over time O(n * log(m * t_i))
include_group gA
include_group gB
include_group gC
include_group gD
tc gE-01 random_generator 1 1000000000 1000000000
tc gE-02 random_generator 5 1000000000 1000000000
tc gE-03 random_generator 10 1000000000 1000000000
tc gE-04 random_generator 100 1000000000 1000000000
tc gE-05 random_generator 1000 1000000000 1000000000
tc gE-06 random_generator 10000 1000000000 1000000000
tc gE-07 random_generator 100000 1000000000 1000000000
tc gE-08 random_generator 200000 1000000000 1000000000
tc gE-09 random_generator 200000 1000000000 123
tc gE-10 random_generator 200000 1000000000 5

